{
  "name": "next-trpc",
  "version": "0.3.1",
  "description": "tRPC + Next.js",
  "keywords": [],
  "author": "Ian Hunter <ian@ianh.xyz>",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ian/next-trpc.git"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.cjs",
    "./client": "./dist/client.cjs",
    "./react": "./dist/react.cjs"
  },
  "typesVersions": {
    "*": {
      "client": [
        "dist/client.d.ts"
      ],
      "react": [
        "dist/react.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --clean --external next --external react src/**/*.ts && tsc --emitDeclarationOnly",
    "build:watch": "nodemon --exec \"pnpm build\""
  },
  "dependencies": {
    "@tanstack/react-query": "^4.10.3",
    "@trpc/client": "10.6.0",
    "@trpc/next": "10.6.0",
    "@trpc/react": "9.27.4",
    "@trpc/react-query": "10.6.0",
    "@trpc/server": "10.6.0",
    "cross-fetch": "^3.1.5",
    "superjson": "^1.12.1"
  },
  "peerDependencies": {
    "next": "^12.1.6",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "17.0.37",
    "next": "^12.1.6",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.6.3"
  }
}
