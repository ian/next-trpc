    "build:watch": "nodemon --exec \"pnpm build\""
  },
  "dependencies": {
    "@tanstack/react-query": "^4.10.1",
    "@trpc/client": "10.0.0-proxy-beta.13",
    "@trpc/next": "10.0.0-proxy-beta.13",
    "@trpc/react": "10.0.0-proxy-beta.13",
    "@trpc/server": "10.0.0-proxy-beta.13"
  },
  "peerDependencies": {
    "next": "^12.1.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "17.0.37",
    "tsup": "^6.2.3",
    "typescript": "^4.6.3"
  }
}
